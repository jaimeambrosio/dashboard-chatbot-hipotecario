<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <title>Dashboard Interbank Bot</title>

  <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/css/bootstrap.min.css">
  <script src="//ajax.googleapis.com/ajax/libs/jquery/2.0.3/jquery.min.js" type="text/javascript"></script>
  <link rel="stylesheet" href="css/dashboard.css">
  <link rel="stylesheet" href="css/fonts.css">
  <link rel="stylesheet" href="css/button.css"> 
  
</head>



<body>

<!-- Start NavBar-->
  <nav class="navbar navbar-inverse navbar-fixed-top" role="navigation">
    <div class="row">
        <div class="navbar-header col-lg-11 col-md-10 col-xs-9 col-sm-10">                
            <a class="navbar-brand" href="#">
                <img class="img-logo" src="images/greenlogo.png">
            </a>

        </div>
        <div class="navbar-header navbar-right col-lg-1 col-md-2 col-xs-3 col-sm-2">
         <a href="/" class="navbar-brand">
           <img class="img-logout" src="images/logout.png">
         </a>
      </div>
    </div>
  </nav>
  <!-- End NavBar -->


  <div class="container">
    <div id="app">
    <!-- Start Contadores Conversaciones y Mensajes-->
      <div class="row">
      <!-- Semanal-->
        <div class="col-md-6">
        <h2 class="sub-header">Semanal</h2> 
          <div class="col-xs-12 col-sm-6 col-md-6">
            <div class="well">
              <h1 id="conversacionesWeek" class="dash-red"></h1>
              <h3 class="text-muted">Conversaciones</h3>
            </div>
          </div>

          <div class="col-xs-12 col-sm-6 col-md-6">
            <div class="well">
              <h1 id="mensajesWeek" class="dash-red"></h1>
              <h3 class="text-muted">Mensajes</h3>
            </div>
          </div>
        </div>
      <!--End Semanal -->

      <!-- Mensual -->
        <div class="col-md-6">
        <h2 class="sub-header">Mensual</h2> 
          <div class="col-xs-12 col-sm-6 col-md-6">
            <div class="well">
              <h1 id="conversacionesMonth" class="dash-red"></h1>
              <h3 class="text-muted">Conversaciones</h3>
            </div>
          </div>

          <div class="col-xs-12 col-sm-6 col-md-6">
            <div class="well">
              <h1 id="mensajesMonth" class="dash-red"></h1>
              <h3 class="text-muted">Mensajes</h3>
            </div>
          </div>
        </div>
      <!--End Mensual -->
      </div>
    <!--End Contadores -->

    
       

    <!--Start Chart Ultima Semana -->
      <div class="row col-xs-12 col-md-7"> 
        <div class="row col-xs-10">
          
            <h2 id="headerChart" class="sub-header">Última Semana</h2> 
          
            <button onclick="cambiarChart()" id="cambiarChart" class="btn btn-primary">Ver Mensual</button>  
            <br/>
          
        </div> 
     
             
          <div id="divSemana" style="margin-top: 120px;">          
            <canvas id="canvasLine" height="200" ></canvas>
          </div>
          <div id="divMes" style="display:none; margin-top: 120px;">          
            <canvas id="canvasLineMes" height="200" ></canvas>
          </div>
      </div>
    <!-- End Chart Ultima Semana -->

    

    <!--Start Chart Top Consultas -->
      <div class="row col-xs-12 col-md-4 col-md-offset-1">
        <h2 class="sub-header">Top 5 Consultas</h2> 
        <div>           
          <canvas id="canvaspie" height="300" ></canvas> 
        </div>
      </div>
    <!--End Chart Top Consultas -->   

    <!-- Start Descargas-->
      <div class="row col-xs-12">
        <h2 class="sub-header">Descargas</h2> 
        <div class="col-xs-6 col-md-3 div-boton" >
          <a href="#" id="descargarWeek" onclick="" class="button_ibk_default">CSV Semanal</a>
        </div>
         <div class="col-xs-6 col-md-3 div-boton" >
          <a href="#" id="descargarMonth" onclick="" class="button_ibk_default">CSV Mensual</a>
        </div>
      </div>
    <!--End Descargas --> 
    </div>
  </div>

  <footer>  
  <div class="bottom-bar footer-bar">
      All Rights Reserved © 2017 | <a href="#">Privacy Policy</a> | <a href="#">Terms of Service</a>
  </div>
</footer>
  
  <script src="/js/public/Chart.min.js"></script>
  <script src="/js/public/papaparse.min.js"></script>
  <script src="/js/contadores.js"></script>
  <script src="/js/descargarcsv.js"></script>
  <script src="/js/topconsulta.js"></script>
  <script src="/js/ultimasemana.js"></script>
  <script src="/js/logmensual.js"></script>
  
</body>
</html>